@model List<ClassRoomTeacherViewModel>
<partial name="_PartialNavbar"></partial>
<div class="row">
	<partial class="col-2" name="_PartialSlidebar"></partial>
	<div class="col-9">
		<div class="container mt-2">
			<h2 class="fw-bold text-center text-secondary mt-2 mb-2">ÖĞRETMEN LİSTESİ</h2>

			<table class="table table-hover mt-3" id="announcementList">
				<thead class="table-info mb-1">
					<tr>
						<th>Adı Soyadı</th>
						<th>Sınıfı</th>

					</tr>
				</thead>

				<tbody>
					@foreach (var teacher in Model)
					{

						<tr>
							<td>@teacher.FullName</td>
							<td>@teacher.Branch</td>

						</tr>
					}


				</tbody>

			</table>

		</div>
	</div>
</div>








